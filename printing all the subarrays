#include<iostream>
using namespace std;
int main(){
    int n;
    cout<<"Enter the number of elements in your array"<<endl;
    cin>>n;
    int arr[n];
    cout<<"Enter your array"<<endl;
    for(int i =0; i<n; i++){
        cin>>arr[i];
  }
  for(int m=0; m<n; m++){
  for(int k=m; k<n; k++){
     for(int l=m; l<=k; l++){
          cout<<arr[l];
}
      cout<<"\n";
  }
}
    
}
