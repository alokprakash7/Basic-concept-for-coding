# include<iostream>
using namespace std;
 int main(){
     string word;
     cout<<"Enter your alphabets"<<endl;
     cin>>word;
     cout<<"Your alphabets has been changed into capital letters"<<endl;
     for(int i =0; i<word.size(); i++){
         if(word[i] >= 'a' && word[i] <= 'z'){
              word[i] -=32;
         }
         cout<<word[i];
     }
     
    
 }
