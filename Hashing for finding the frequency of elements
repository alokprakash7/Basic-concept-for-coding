# include<bits/stdc++.h>
using namespace std;
void Frequency(int arr[], int n){
    unordered_map<int, int> map;
    for(int i =0; i<n; i++)
        map[arr[i]]++;
    for (auto x : map ){
        cout<<x.first<<" "<<x.second<<endl;
    }
}
int main(){
    cout<<"Enter the length of the array"<<endl;
    int n;
    cin>>n;
    int arr[n];
    cout<<"Enter your array "<<endl;
for(int i =0; i<n; i++){
    cin>>arr[i];
}
Frequency(arr, n);
return 0;
    }
