# include <iostream>
using namespace std;
int main(){
    cout<<"Enter the value of m and n for your matrix"<<endl;
    int m,n;
     cin>>m>>n;
     int arr[m][n];
     cout<<"Enter your elements which you want to represent in matrix form"<<endl;
     for(int i = 0; i<m; i++){
         for(int j =0; j<n; j++){
             cin>>arr[i][j];
         }
     }
     cout<<"Enter the key element of which you want to find the coordinates"<<endl;
     int key;
     cin>>key;
     for(int i =0; i<n; i++){
         for(int j =0; j<n; j++){
             if(key == arr[i][j]){
                 cout<<"The coordinates of your key element is "<<i<<","<<j<<endl;
             }
         }
     }
}
