# include<iostream>
using namespace std;
void swap(int arr[], int n){
    int temp = arr[1];
    arr[1] = arr[n-1];
    arr[n-1] = temp;
}
int main(){
    int n;
    cout<<"Enter the number of elements"<<endl;
    cin>>n;
    int arr[n];
    for(int i = 0; i<n; i++){
        cin>>arr[i];
    
   
    }
     swap(arr[1], arr[n-1]);
     cout<<"Here is your answer"<<endl;
    for(int k = 0; k<n; k++){
    cout<<arr[k]<<endl;
    }
}
