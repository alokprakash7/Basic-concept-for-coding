# include <iostream>
using namespace std;
int main(){
    
    char arr[100];
    cout<<"Enter your word"<<endl;
    cin>>arr;
    cout<<"Enter the character of which you want to find the index"<<endl;
    char key;
    cin>>key;
    for(int i =0; i<99; i++){
        if(key == arr[i]){
            cout<<i<<endl;
        }
    }
}
